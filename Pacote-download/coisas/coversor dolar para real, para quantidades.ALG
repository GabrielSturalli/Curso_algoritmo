Algoritmo "CONVERSORMOEDAS"

Var
c, q: inteiro
R, D: REAL

Inicio
      C <- 1
      escreva("quantas vezes você quer converter? ")
      leia(Q)
      ENQUANTO (C<=Q) FACA
               ESCREVAL("Qual o valor em R$? ")
               leia (R)
                D <- R/6.20
                 ESCREVAl("O valor convertido e US$", D:5:2)
                  C<- C+1
      FIMENQUANTO

Fimalgoritmo